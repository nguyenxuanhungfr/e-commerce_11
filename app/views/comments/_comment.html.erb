<div class="col-md-1">
  <div class="thumbnails">
    <%= get_user_images comment %>
  </div>
</div>
<div class="col-md-11">
  <div class="panel panel-default">
    <div class="panel-heading">
      <strong><%= comment.user.name %></strong>
      <span class="text-muted"><span class="text-muted"><%= time_ago_in_words comment.created_at %></span></span>
    </div>
    <div class="panel-body">
      <%= comment.description %>
    </div>
  </div>
</div>
